{
  "openapi": "3.0.0",
  "info": {
    "version": "1.0.0",
    "title": "coc",
    "description": "real-time map"
  },
  "tags": [
    {
      "name": "user",
      "description": "user api"
    }
  ],
  "servers": [
    {
      "description": "coc",
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/user/register": {
      "post": {
        "tags": ["user"],
        "summary": "회원가입",
        "requestBody": {
          "description": "유저 회원가입",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "accountId": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  },
                  "name": {
                    "type": "string"
                  },
                  "isUser": {
                    "type": "boolean"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "성공"
          }
        }
      }
    },
    "/user/login": {
      "post": {
        "tags": ["user"],
        "summary": "로그인",
        "requestBody": {
          "description": "유저 로그인",
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "accountId": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "성공",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "error": {
                      "type": "boolean"
                    },
                    "token": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
